{
  "vcpkg-configuration": {
    "default-registry": {
      "kind": "git",
      "baseline": "b42b19a62b63514ed9fa2a81e07ce75e613c83c0",
      "repository": "https://github.com/microsoft/vcpkg"
    },
    "registries": [
      {
        "kind": "git",
        "baseline": "2d2485873f139e16ff09169bc3a3274ff08048c0",
        "repository": "https://github.com/open-vcpkg/python-registry",
        "packages": [
          "python3",
          "py-*",
          "gdal",
          "vcpkg-python-scripts",
          "vcpkg-tool-mercurial",
          "vcpkg-tool-rust",
          "pybind11"
        ]
      }
    ],
    "overlay-ports": [
      "ports"
    ],
    "overlay-triplets": [
      "triplets"
    ]
  },
  "name": "kadas-albireo2",
  "version-string": "current",
  "description": "KADAS Albireo is a mapping application based on QGIS and targeted at non-specialized users, providing enhanced functionalities in areas such as drawing, measuring, terrain analysis, etc.",
  "homepage": "https://github.com/kadas-albireo/kadas-albireo2",
  "dependencies": [
    "cairo",
    {
      "name": "ffmpeg",
      "features": [
        "ffmpeg",
        "ffprobe"
      ],
      "platform": "windows"
    },
    "geographiclib",
    "librsvg",
    "proj-chenyx06a",
    "py-psycopg2",
    "python3",
    {
      "name": "qgis",
      "features": [
        "3d",
        "gui"
      ]
    },
    "quazip",
    "zonedetect"
  ],
  "features": {
    "bindings": {
      "description": "Build with python bindings",
      "dependencies": [
        {
          "name": "gdal",
          "features": [
            "python"
          ]
        },
        "py-autopep8",
        "py-charset-normalizer",
        "py-dateutil",
        "py-ephem",
        "py-flask",
        "py-geographiclib",
        "py-isort",
        "py-jinja2",
        {
          "name": "py-kerberos-proxy-auth",
          "platform": "windows"
        },
        "py-lxml",
        "py-markupsafe",
        {
          "name": "py-matplotlib",
          "platform": "windows"
        },
        {
          "name": "py-numpy",
          "platform": "windows"
        },
        {
          "name": "py-opencv-contrib-python",
          "platform": "windows"
        },
        "py-owslib",
        "py-pip",
        "py-pip-system-certs",
        "py-psycopg2",
        "py-pyqt6",
        "py-pytz",
        "py-pyyaml",
        "py-qscintilla",
        {
          "name": "py-requests-kerberos",
          "platform": "windows"
        },
        "py-six",
        "py-waitress",
        {
          "name": "qgis",
          "features": [
            "bindings",
            "desktop"
          ]
        }
      ]
    }
  },
  "overrides": [
    {
      "name": "libspatialindex",
      "version": "2.0.0"
    }
  ]
}
