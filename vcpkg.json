{
  "name": "kadas-albireo2",
  "version-string": "current",
  "description": "KADAS Albireo is a mapping application based on QGIS and targeted at non-specialized users, providing enhanced functionalities in areas such as drawing, measuring, terrain analysis, etc.",
  "homepage": "https://github.com/kadas-albireo/kadas-albireo2",
  "dependencies": [
    {
      "name": "qgis",
      "features": [
        "gui"
      ]
    },
    "geographiclib",
    "quazip-qt5",
    "cairo",
    "librsvg",
    "svg2svgt-qt5",
    "python3",
    "zonedetect",
    {
      "name": "qt5-activeqt",
      "platform": "windows"
    }
  ],
  "features": {
    "globe": {
      "description": "Globe",
      "dependencies": ["osgearth"]
    }
  },
  "vcpkg-configuration": {
    "default-registry": {
      "kind": "git",
      "baseline": "fecdd89f28ed883b26a48c2b30085f6384070bb8",
      "repository": "https://github.com/microsoft/vcpkg"
    },
    "registries": [
      {
        "kind": "git",
        "baseline": "c288fd01cbc95c56a122747c3c39cbab2710ee17",
        "repository": "https://github.com/open-vcpkg/vcpkg-qt5",
        "packages": [
          "qscintilla-qt5",
          "qt5-webkit"
        ]
      }
    ],
    "overlay-ports": [
      "vcpkg/ports"
    ],
    "overlay-triplets": [
      "vcpkg/triplets"
    ]
  }
}
