{
  "vcpkg-configuration": {
    "default-registry": {
      "kind": "git",
      "baseline": "ce1916404fc6f2b645f419a6d47b7ebafe686582",
      "repository": "https://github.com/microsoft/vcpkg"
    },
    "registries": [
      {
        "kind": "git",
        "baseline": "ff79a0956d18156e493b3c1f6342fd01fe563283",
        "repository": "https://github.com/open-vcpkg/qt5-registry",
        "packages": [
          "py-pyqt5",
          "py-pyqt5-sip",
          "py-qscintilla-qt5",
          "qscintilla-qt5",
          "qt5-webkit",
          "vcpkg-qmake-qt5"
        ]
      },
      {
        "kind": "git",
        "baseline": "719f4a42d3bb6b573503adf50c9216a620cd579f",
        "repository": "https://github.com/open-vcpkg/python-registry",
        "packages": [
          "python3",
          "py-*",
          "vcpkg-python-scripts"
        ]
      }
    ],
    "overlay-ports": [
      "vcpkg/ports"
    ],
    "overlay-triplets": [
      "vcpkg/triplets"
    ]
  },
  "name": "kadas-albireo2",
  "version-string": "current",
  "description": "KADAS Albireo is a mapping application based on QGIS and targeted at non-specialized users, providing enhanced functionalities in areas such as drawing, measuring, terrain analysis, etc.",
  "homepage": "https://github.com/kadas-albireo/kadas-albireo2",
  "dependencies": [
    "cairo",
    "geographiclib",
    "librsvg",
    "python3",
    {
      "name": "qgis",
      "features": [
        "gui"
      ]
    },
    {
      "name": "qt5-activeqt",
      "platform": "windows"
    },
    "quazip-qt5",
    "svg2svgt-qt5",
    "zonedetect"
  ],
  "features": {
    "bindings": {
      "description": "Build with python bindings",
      "dependencies": [
        "py-pip",
        "py-kadas-requirements",
        {
          "name": "qgis",
          "features": [
            "bindings"
          ]
        },
        {
          "name": "gdal",
          "features": [
            "python"
          ]
        }
      ]
    },
    "globe": {
      "description": "Globe",
      "dependencies": [
        "osgearth"
      ]
    },
    "webkit": {
      "description": "Build with Qt Webkit",
      "dependencies": [
        {
          "name": "qgis",
          "features": [
            "webkit"
          ]
        }
      ]
    }
  }
}
